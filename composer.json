{
    "name": "erebot/wordlists-module",
    "description": "Erebot module that provides lists of words for other modules",
    "keywords": ["Erebot", "IRC", "bot", "module"],
    "homepage": "https://github.com/Erebot/Erebot_Module_Wordlists",
    "license": "GPL-3.0+",
    "authors": [
